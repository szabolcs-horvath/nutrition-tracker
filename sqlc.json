{
  "version": "2",
  "sql": [
    {
      "engine": "sqlite",
      "database": {
        "uri": "${SQLITE3_DB_STRING}"
      },
      "schema": "sqlite/migrations",
      "queries": "sqlite/queries",
      "gen": {
        "go": {
          "package": "sqlc",
          "out": "generated",
          "emit_pointers_for_null_types": true,
          "emit_empty_slices": true,
          "rename": {
            "user": "User_sqlc",
            "item": "Item_sqlc",
            "nutrition": "Nutrition_sqlc",
            "notification": "Notification_sqlc"
          }
        }
      }
    }
  ],
  "overrides": {
    "go": {
      "overrides": [
        {
          "db_type": "INTEGER",
          "nullable": true,
          "go_type": {
            "type": "*int64",
          }
        },
        {
          "db_type": "INTEGER",
          "nullable": false,
          "go_type": {
            "type": "int64",
          }
        },
        {
          "db_type": "TIME",
          "nullable": true,
          "engine": "sqlite",
          "go_type": {
            "type": "*time.Time",
          }
        },
        {
          "db_type": "TIME",
          "nullable": false,
          "engine": "sqlite",
          "go_type": {
            "type": "time.Time",
          }
        }
      ]
    }
  }
}